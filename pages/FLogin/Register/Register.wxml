<!--pages/FLogin/Register/Register.wxml-->
<view class="box flex">

	<view class='logo'>
		<image src='../../../images/logo1.png'></image>
	</view>
	<view>
		<form bindsubmit="formsubmit">
			<view class="flex login">
				<input placeholder=">>请输入账号" class="inp" name="fixId"></input>
				<input placeholder=">>请输入密码" class="inp" type="password" name="pass">
				</input>
				<input placeholder=">>请确认密码" class="inp" type="password" name="confirmPass">
				</input>
        <input placeholder=">>请输入手机号码" class="inp"  name="tel">
				</input>
        <input placeholder=">>请输入真实姓名" class="inp" name="realName">
				</input>
			</view>
			<view class="flex lr">

				<view class="padding-xl">
					<button class="bg-cyan " catchtap="handRegister" form-type="submit">确认注册
					</button>
				</view>
			</view>

		</form>
	</view>




</view>